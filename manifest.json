{
  "name": "SG Timesheet",
  "version": "0.0.8.2",
  "manifest_version": 2,
  "description": "Timesheet helper",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "SG Timesheet",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "contentSettings",
    "notifications",
    "alarms",
    "storage",
    "https://kz7jvrkz59.execute-api.us-west-1.amazonaws.com/dev/SGTimesheet/"
  ],
  "background": {
    "scripts": ["src/background.js"]
  }
}